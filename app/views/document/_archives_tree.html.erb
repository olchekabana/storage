<ul class="item-ul">
<% 	
	arr = [0]
	sub_id = -1
 	
	@tree.each do |contract|
		if contract.sub_id == sub_id
			arr << sub_id
			%><ul><%
		else
			until contract.sub_id == arr.last do
				arr.pop
				%></ul><%
			end
		end
		%><li><span>
			<%= link_to_unless(contract.id == id.to_i, contract.name, "/#{model}/#{contract.id}") do
				content_tag(:span, contract.name, class: "span")
			end %>
		</span><%
		sub_id = contract.id
	end
%>
</ul>
